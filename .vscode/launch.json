	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch CLI Lexer",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/OSX/Lib/CLI.o",
			"args": ["-target", "lexer"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"preLaunchTask": "buildCli",
			"MIMode": "lldb"
		},
		{
			"name": "Launch CLI Parser",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/OSX/Lib/CLI.o",
			"args": ["-target", "parser"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"preLaunchTask": "buildCli",
			"MIMode": "lldb"
		}
	]
}